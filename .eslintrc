{
  "extends": [ "eslint:recommended" ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["babel"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info"] }]
  }
}