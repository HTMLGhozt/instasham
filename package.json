    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.90.0",
    "html-webpack-plugin": "^3.2.0",
